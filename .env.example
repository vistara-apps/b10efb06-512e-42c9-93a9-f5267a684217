# OnChainKit API Key (required)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# Privy App ID (required for wallet authentication)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Base RPC URLs (optional - defaults provided)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Alchemy RPC URL (optional - for enhanced blockchain data)
NEXT_PUBLIC_ALCHEMY_RPC_URL=https://base-mainnet.g.alchemy.com/v2/your_api_key

# Environment
NODE_ENV=development
